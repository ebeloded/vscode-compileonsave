{
  "name": "tsconfig-compileonsave",
  "displayName": "tsconfig: compileOnSave ",
  "description": "Integrated compilation of TypeScript projects",
  "version": "1.0.0",
  "publisher": "Evgenij Beloded",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Productivity",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "strip-json-comments": "^2.0.1",
    "tree-kill": "^1.1.0"
  }
}